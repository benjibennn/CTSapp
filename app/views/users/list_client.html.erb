<div class="show-client"> <h1> Display all Clients </h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name </th>
    <th>Gender </th>
    <th>Phone </th>
  </tr> 

  <% @client.each do |client| %>

	  <tr>
	    <td><%= client.first_name%></td>
	    <td><%= client.last_name%></td>
	    <td><%= client.gender%></td>
	    <td><%= client.phone%></td>
	  </tr>
  <% end %>
</table>

<!--  -->
<a href="/users/<%= current_user.id%>/create_teacher"> Create Client </a>
</div>