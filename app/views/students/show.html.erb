<p id="notice"><%= notice %></p>
<table>

<tr>
<td>First Name</td>
<td>Last Name</td>
<td>Gender</td>
</tr>

<tr>
<td><%=@student.first_name%></td>
<td><%=@student.last_name%></td>
<td><%=@student.gender%></td>

</tr>
</table>

<h4>Classes taken</h4>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Level</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @classes.each do |x| %>
      <tr>
        <td><%= Subject.find(x.subject_id).subject_name %></td>
        <td><%= Subject.find(x.subject_id).level %></td>
        <td><%= Subject.find(x.subject_id).start_time %></td>
        <td><%= Subject.find(x.subject_id).end_time %></td>

      </tr>
    <% end %>
  </tbody>
</table>
<a href="/events">Show Calendar</a>
